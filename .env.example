# OpenAI
# Recommended for quality/cost balance: gpt-4.1-mini.
# For max quality (higher cost/latency): gpt-4.1
EXPO_PUBLIC_OPENAI_MODEL=gpt-4.1-mini
OPENAI_MODEL=gpt-4.1-mini
OPENAI_TEMPERATURE=0.2
OPENAI_MAX_TOKENS=200
OPENAI_API_KEY=

# API base URL used by the mobile client.
# For phone builds (including EAS/internal testing), set this to a backend URL the device can reach.
# Avoid localhost for physical devices unless you are intentionally using a tunnel/dev networking workaround.
EXPO_PUBLIC_API_BASE_URL=

# RevenueCat public SDK keys
# For this release pipeline, Android key is required.
EXPO_PUBLIC_REVENUECAT_ANDROID_API_KEY=goog_xxxxxxxxxxxxxxxxxxxxx
EXPO_PUBLIC_REVENUECAT_IOS_API_KEY=appl_xxxxxxxxxxxxxxxxxxxxx

# RevenueCat product setup
EXPO_PUBLIC_RC_ENTITLEMENT_ID=unlimited_messages
EXPO_PUBLIC_RC_OFFERING_ID=default
EXPO_PUBLIC_RC_MONTHLY_PRODUCT_ID=monthly
